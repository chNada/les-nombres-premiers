#include <stdio.h>

int main() {
    int a,b,i;
    printf("Try: ");
    scanf("%d",&a);

    for(b = 2; b <= a; b++) {

        int isPrime = 1;

        for(i = 2; i*i <= b; i++) {  
            if(b % i == 0) {  
                isPrime = 0;  
                break;
            }
        }

        if(isPrime == 1) {
            printf("%d est premier\n", b);
        }
    }

    return 0;
}
